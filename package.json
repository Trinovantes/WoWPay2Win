        "@types/copy-webpack-plugin": "^8.0.0",
        "@types/lodash": "^4.14.165",
        "@types/mini-css-extract-plugin": "^1.4.1",
        "@types/node": "^14",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^3.11.3",
        "@types/webpack-env": "^1.15.3",
