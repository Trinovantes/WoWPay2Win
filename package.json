{
    "private": true,
    "scripts": {
        "fetchItems": "   node -r source-map-support/register dist/cron/fetchItems.js    src/web/client/assets/data src/web/client/assets/img",
        "fetchAuctions": "node -r source-map-support/register dist/cron/fetchAuctions.js src/web/client/assets/data dist/web/data",
        "devCron": "  cross-env NODE_ENV=development webpack --watch --config build/webpack.config.cron.ts",
        "devWeb": "   cross-env NODE_ENV=development webpack serve   --config build/webpack.config.web.ts",
        "buildCron": "cross-env NODE_ENV=production  webpack         --config build/webpack.config.cron.ts",
        "buildWeb": " cross-env NODE_ENV=production  webpack         --config build/webpack.config.web.ts",
        "dev": "  concurrently 'yarn devCron'   'yarn devWeb'",
        "build": "concurrently 'yarn buildCron' 'yarn buildWeb'",
        "lint": "vue-tsc --noemit && eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
        "analyze": "WEBPACK_ANALYZE=true NODE_ENV=production webpack --config build/webpack.config.web.ts"
    },
    "dependencies": {
        "source-map-support": "^0.5.19"
    },
    "devDependencies": {
        "@sentry/node": "^7.2.0",
        "@sentry/tracing": "^7.2.0",
