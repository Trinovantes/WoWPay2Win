{
    "private": true,
    "scripts": {
        "fetchItems": "   node -r dotenv/config dist/cron/fetchItems.js",
        "fetchAuctions": "node -r dotenv/config dist/cron/fetchAuctions.js",
        "devCron": "  cross-env NODE_ENV=development webpack --watch    --config build/webpack.config.cron.ts",
        "devWeb": "   cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.web.ts",
        "buildCron": "cross-env NODE_ENV=production  webpack            --config build/webpack.config.cron.ts",
        "buildWeb": " cross-env NODE_ENV=production  webpack            --config build/webpack.config.web.ts",
        "dev": "  concurrently 'yarn devCron'   'yarn devWeb'",
        "build": "concurrently 'yarn buildCron' 'yarn buildWeb'",
        "lint": "eslint --ext .js,.ts,.vue ."
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
        ]
    },
    "dependencies": {
        "axios": "^0.21.1",
        "concurrently": "^5.3.0",
        "dayjs": "^1.9.5",
        "dotenv": "^8.2.0",
        "lodash": "^4.17.20",
        "material-design-icons-iconfont": "^6.1.0",
        "quasar": "^1.14.3",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.0.2",
        "vue-router": "^3.4.9",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/register": "^7.12.1",
        "@types/copy-webpack-plugin": "^6.3.0",
        "@types/lodash": "^4.14.165",
        "@types/mini-css-extract-plugin": "^1.2.2",
        "@types/node": "^14.11.7",
        "@types/webpack-dev-server": "^3.11.1",
        "@types/webpack-env": "^1.15.3",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.2.0",
        "copy-webpack-plugin": "^6.3.2",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.0",
        "eslint": "^7.9.0",
